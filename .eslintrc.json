{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "linebreak-style": 0,
        "quotes": [
            1,
            "single"
        ],
        "no-unused-vars": "warn",
        "indent": [
            "error",
            4,
            {
                "SwitchCase": 1
            }
        ],
        "no-plusplus": 0,
        "no-console": 0,
        "no-multiple-empty-lines": 0,
        "max-len": 0,
        "prefer-const": 1,
        "import/extensions": 0,
        "no-restricted-syntax": 1,
        "default-case": 1,
        "space-infix-ops": 1,
        "comma-spacing": 1,
        "comma-dangle": 1,
        "radix": 1,
        "eqeqeq": 1,
        "no-unused-expressions": 1
    }
}
